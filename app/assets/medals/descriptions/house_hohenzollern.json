  "id": "ordem_casa_hohenzollern",
  "localizations": {
    "pt-BR": {
      "nome": "Ordem da Casa de Hohenzollern",
      "nomeOriginal": "Hausorden von Hohenzollern",
      "resumo": "Uma ordem dinástica de cavalaria da Casa de Hohenzollern, concedida a oficiais militares comissionados e civis de status comparável. Estavam associadas à ordem diversas cruzes e medalhas para soldados e civis de patente inferior."
    },
    "en-US": {
      "nome": "House Order of Hohenzollern",
      "nomeOriginal": "Hausorden von Hohenzollern",
      "resumo": "A dynastic order of knighthood of the House of Hohenzollern, awarded to commissioned military officers and civilians of comparable status. Various crosses and medals were associated with the order for soldiers and civilians of lower rank."
    },
    "de-DE": {
      "nome": "Hausorden von Hohenzollern",
      "nomeOriginal": "Hausorden von Hohenzollern",
      "resumo": "Ein dynastischer Ritterorden des Hauses Hohenzollern, verliehen an beauftragte Militäroffiziere und Zivilisten vergleichbaren Status. Verschiedene Kreuze und Medaillen waren mit dem Orden für Soldaten und Zivilisten niedrigeren Ranges verbunden."
    }
  },
